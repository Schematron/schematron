<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="doc">
		<xsd:complexType>
			<xsd:choice>
				<xsd:element name="elem" type="elem"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="elem">
		<xsd:sequence>
			<xsd:any minOccurs="2" maxOccurs="1000000" processContents="skip"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
