<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:appinfo>Application Information</xsd:appinfo>
		<xsd:documentation>Documentation</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="thing" type="xsd:string">
		<xsd:annotation>
			<xsd:appinfo>Application Information</xsd:appinfo>
			<xsd:documentation>Documentation</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="myComplexType">
		<xsd:annotation>
			<xsd:appinfo>Application Information</xsd:appinfo>
			<xsd:documentation>Documentation</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:annotation>
				<xsd:appinfo>Application Information</xsd:appinfo>
				<xsd:documentation>Documentation</xsd:documentation>
			</xsd:annotation>
			<xsd:element ref="thing"/>
		</xsd:all>
	</xsd:complexType>
</xsd:schema>
