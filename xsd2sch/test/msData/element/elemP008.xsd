<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="myElem" type="xsd:string"/>
	<xsd:complexType name="mytype">
		<xsd:all>
			<xsd:element ref="myElem">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer"/>
				</xsd:simpleType>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
</xsd:schema>
