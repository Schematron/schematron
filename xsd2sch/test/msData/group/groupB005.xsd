<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:complexType name="complexType">
		<xsd:choice>
			<xsd:group ref="g1" minOccurs="2" maxOccurs="2"/>
			<xsd:group ref="g2" minOccurs="2" maxOccurs="100"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:element name="elem" type="complexType"/>
	<xsd:element name="doc">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="elem"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:group name="g1">
		<xsd:sequence>
			<xsd:element name="r1"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="g2">
		<xsd:sequence>
			<xsd:element name="r2"/>
		</xsd:sequence>
	</xsd:group>
</xsd:schema>
