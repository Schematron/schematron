<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

<!-- include two identical schemas(different name) with unique Ids   -->

<xsd:complexType id="inc_addr" name="Address1">
	<xsd:sequence>
		<xsd:element name="name"   type="xsd:string"/>
		<xsd:element name="street" type="xsd:string"/>
		<xsd:element name="city"   type="xsd:string"/>
	</xsd:sequence>
</xsd:complexType>

</xsd:schema>
