<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:simpleType name="myTest">
		<xsd:restriction base="xsd:integer">
			<xsd:pattern value="[0-9]{1,5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="fooType">
		<xsd:list itemType="myTest"/>
	</xsd:simpleType>
</xsd:schema>
